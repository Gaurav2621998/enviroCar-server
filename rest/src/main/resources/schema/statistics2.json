{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://schema.envirocar.org/statistics2.json#",
    "type": "object",
    "additionalProperties": false,
    "required": [
		"user",
		"distance",
		"duration",
		"below60kmh",
		"above130kmh",
		"trackSummaries"
    ],
    "properties": {
		"user": {
			"$ref": "http://schema.envirocar.org/user.json#"
		},
		"distance": {
			"type": "number"
		},
		"duration": {
			"type": "number"
		},
		"statistics2": {
			"below60kmh": {
				"distance": {
					"type": "number"
				},
				"duration": {
					"type": "number"
				}
			},
			"above130kmh": {
				"distance": {
					"type": "number"
				},
				"duration": {
					"type": "number"
				}
			}
		},
		"trackSummaries":{
			"$ref": "http://schema.envirocar.org/trackSummaries.json#"
		}
    }
}